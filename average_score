grades = [[5, 3, 3, 5, 4], [2, 2, 2, 3], [4, 5, 5, 2], [4, 4, 3], [5, 5, 5, 4, 5]]
students = {'Johnny', 'Bilbo', 'Steve', 'Khendrik', 'Aaron'}
students = list(students)
students.sort()
list_average_score = [round(sum(average_score)/len(average_score), 2) for average_score in grades]
dict_average_score = dict(list(zip(students, list_average_score)))
print('Средний балл:', dict_average_score)
